SHELL=/bin/sh
AR=@AR@
RANLIB=@RANLIB@
GUILECONF=@GUILECONF@

TOPDIR=@top_srcdir@

MLANLIB=mlan/mlan.a

VPATH=$(TOPDIR):.

CC=@CC@

.SUFFIXES: .c .o .l .a .ln

all:
	cd mlan && make
	cd commands && make
	cd collection && make

distclean: clean
	cd collection && make clean
	rm -f Makefile config.cache config.status config.log \
		collection/Makefile mlan/Makefile

clean:
	rm -f $(PROGS) $(PROGO)
	cd mlan && make clean
	cd commands && make clean
	cd collection && make clean
