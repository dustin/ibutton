SHELL=/bin/sh
AR=@AR@
RANLIB=@RANLIB@

DEFS=@DEFS@
CFLAGS=-I. @CFLAGS@ $(DEFS)

OBJ=com.o ds2480.o mlan.o devices.o
PROGS=search lookup sample_devices slew
PROGO=search.o lookup.o sample_devices.o slew.o
CC=@CC@

all: $(PROGS)

slew: mlan.a slew.o
	$(CC) -o $@ slew.o mlan.a

sample_devices: mlan.a sample_devices.o
	$(CC) -o $@ sample_devices.o mlan.a

search: mlan.a search.o
	$(CC) -o $@ search.o mlan.a

lookup: mlan.a lookup.o
	$(CC) -o $@ lookup.o mlan.a

mlan.a: $(OBJ) mlan.h
	$(AR) rcs $@ $(OBJ)
	$(RANLIB) $@

com.o: mlan.h

ds2480.o: mlan.h

mlan.o: mlan.h

devices.o: mlan.h

distclean: clean
	rm -f Makefile config.cache config.status config.log

clean:
	rm -f $(OBJ) mlan.a $(PROGS) $(PROGO)
